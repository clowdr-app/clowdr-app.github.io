{
  "name": "clowdr-site",
  "private": true,
  "description": "Midspace (formerly Clowdr) resources website",
  "version": "0.1.0",
  "scripts": {
    "build:dev": "cross-env NODE_ENV=development gatsby build",
    "build:prod": "gatsby clean && gatsby build",
    "build": "cross-env NODE_ENV=production npm run build:prod",
    "clean": "gatsby clean",
    "codegen": "graphql-codegen --config graphql-codegen.config.js",
    "forestry:install": "npm install -g npm@latest && npm install --legacy-peer-deps",
    "forestry:preview": "npm run start -- -p 8080",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "cross-env NODE_ENV=development gatsby develop",
    "serve": "gatsby serve"
  },
  "dependencies": {
    "@chakra-ui/gatsby-plugin": "^2.0.2",
    "@chakra-ui/icons": "^1.1.1",
    "@chakra-ui/live-region": "^1.1.1",
    "@chakra-ui/react": "^1.7.2",
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "cross-env": "^7.0.3",
    "escape-string-regexp": "^4.0.0",
    "fake-tag": "^3.0.0",
    "framer-motion": "^5.3.3",
    "gatsby": "^4.2.0",
    "gatsby-plugin-cname": "^1.0.0",
    "gatsby-plugin-extract-schema": "^0.2.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-image": "^2.2.0",
    "gatsby-plugin-local-search": "^2.0.1",
    "gatsby-plugin-manifest": "^4.2.0",
    "gatsby-plugin-meta-redirect": "^1.1.1",
    "gatsby-plugin-react-helmet-async": "^1.2.0",
    "gatsby-plugin-remark-collection": "^0.1.0",
    "gatsby-plugin-sharp": "^4.2.0",
    "gatsby-plugin-typescript": "^4.2.0",
    "gatsby-remark-autolink-headers": "^5.7.0",
    "gatsby-remark-copy-linked-files": "^5.2.0",
    "gatsby-remark-embed-video": "^3.1.1",
    "gatsby-remark-images": "^6.2.0",
    "gatsby-remark-prismjs": "^6.2.0",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-remark-responsive-iframe": "^5.2.0",
    "gatsby-remark-smartypants": "^5.2.0",
    "gatsby-remark-table-of-contents": "^2.0.0",
    "gatsby-source-filesystem": "^4.2.0",
    "gatsby-source-graphql": "^4.2.0",
    "gatsby-transformer-remark": "^5.2.0",
    "gatsby-transformer-sharp": "^4.2.0",
    "graphql": "^15.7.2",
    "graphql-config": "^4.1.0",
    "path-browserify": "^1.0.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "prismjs": "^1.25.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.1.2",
    "react-icons": "^4.3.1",
    "react-lunr": "^1.1.0",
    "rehype-react": "^7.0.3",
    "unified": "^10.1.1",
    "url-breadcrumb": "^1.0.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/introspection": "^2.1.0",
    "@graphql-codegen/plugin-helpers": "^2.3.1",
    "@graphql-codegen/typescript": "^2.4.1",
    "@graphql-codegen/typescript-operations": "^2.2.1",
    "@hot-loader/react-dom": "^17.0.1",
    "@types/graphql": "^14.5.0",
    "@types/react-helmet": "^6.1.4",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "babel-eslint": "^10.0.0",
    "eslint": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "2.5.0",
    "typescript": "^4.5.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
