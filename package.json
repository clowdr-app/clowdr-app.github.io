    "babel-eslint": "^10.0.0",
    "eslint": "^8.15.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "force-resolutions": "^1.0.10",
    "prettier": "2.6.2",
    "typescript": "^4.6.4"
  },
  "resolutions": {
    "graphql": "15.8.0",
    "source-map": "0.8.0-beta.0"
